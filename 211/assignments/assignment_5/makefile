cppo = main.o
datagramo = datagram.o
systemo = system_utilities.o
objects = $(cppo) $(datagramo) $(systemo)
sources=$(objects:.o=.cpp)
Output = Assignment5
CXX = g++
CXXFLAGS= -std=c++0x

all: $(Output)

$(Output) : $(objects)
	$(CXX) $^ -o $@

clean:
	-rm $(Output) $(objects)
.PHONY : clean

include $(sources:.cpp=.d)

